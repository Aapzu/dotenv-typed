{
  "name": "@aapzu/tsdotenv",
  "version": "0.0.1",
  "description": "Creates and parses a typed dotenv config from a given schema",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "tsc --noEmit && npm run prettier && npm run eslint",
    "prepack": "npm run build"
  },
  "keywords": [
    "dotenv",
    "typescript",
    "strongly-typed"
  ],
  "author": "Aapeli Haanpuu",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.6.3",
    "jest-runner-tsd": "^1.1.0",
    "prettier": "^2.2.1",
    "tmp": "^0.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tsd": "^0.14.0",
    "typescript": "^4.2.4"
  },
  "tsd": {
    "directory": "test",
    "compilerOptions": {
      "strict": true
    }
  }
}
