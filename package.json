{
  "name": "@aapzu/tsdotenv",
  "version": "1.5.0",
  "description": "Creates and parses a typed dotenv config from a given schema",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "npm run test-types && jest",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "ts-compile": "tsc --noEmit",
    "test-types": "tsc --noEmit --project test.tsconfig.json",
    "lint": "npm run ts-compile && npm run prettier && npm run eslint",
    "prepack": "npm run build"
  },
  "keywords": [
    "dotenv",
    "typescript",
    "strongly-typed"
  ],
  "author": {
    "name": "Aapeli Haanpuu",
    "email": "aapzu@iki.fi"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "tmp": "^0.2.1",
    "ts-expect": "^1.3.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "tsd": {
    "directory": "test",
    "compilerOptions": {
      "strict": true
    }
  }
}
