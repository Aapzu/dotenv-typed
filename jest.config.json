{
  "projects": [
    {
      "displayName": "unit",
      "testRegex": ".*(?<!integration)\\.test\\.ts$",
      "setupFiles": ["<rootDir>/test/mocks.ts"],
      "preset": "ts-jest",
      "setupFilesAfterEnv": ["<rootDir>/test/setupTests.ts"]
    },
    {
      "displayName": "integration",
      "testRegex": ".*\\.integration\\.test\\.ts$",
      "preset": "ts-jest",
      "setupFilesAfterEnv": ["<rootDir>/test/setupTests.ts"]
    }
  ]
}
